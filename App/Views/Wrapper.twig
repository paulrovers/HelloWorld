    </style>
    <script type="text/javascript">
        function toggle_menu() {
            let menu = document.querySelector('#menu-navigation');
            let items = document.querySelectorAll('#menu-navigation li');

            if(menu.style.display != 'block') {
                set_items(items,'none');
                menu.style.display = 'block';
            }else{
                set_items(items,'left');
                menu.style.display = 'none';
            }
        }

        function set_items(elementList,listValue)
        {
            let index = 0;
            let length = elementList.length;
            for (; index < length; index++) {
                elementList[index].style.float = listValue;
            }
        }
    </script>
</head>

<body>

<header id="masthead" class="site-header">
    <div class="inside-header grid-container grid-parent">
        <div class="site-branding">
            <p class="main-title">
                <a href="/" rel="home">
                    Hello World
                </a>
            </p>
            <p class="site-description">
                The easiest way to create a new website
            </p>
        </div>
    </div>
</header>

<nav id="site-navigation" class="main-navigation">
    <div class="inside-navigation grid-container grid-parent">
        <button class="menu-toggle" onclick="toggle_menu()">
            <span><img src="/images/hamburger.svg" id="hamburger"></span>
            <span class="mobile-menu">Menu</span>
        </button>
        <div id="primary-menu" class="main-nav">
            <ul id="menu-navigation">
                <li><a href="/">Home</a></li>
                <li><a href="/contact/">Contact</a></li>
                <li><a href="/login/">Login</a></li>
            </ul>
        </div>
    </div>
</nav>

<div id="page" class="site grid-container container hfeed grid-parent">
    <div id="content" class="site-content">
        <div id="primary">
            <main id="main" class="site-main">
                    <div style="margin:20px">
                        <header class="entry-header">
                            <h1 class="entry-title">{{ h1_title }}</h1>
                        </header>
                        <div class="entry-content">
                            {{ subpage | raw }}
                        </div>
                    </div>
            </main>
        </div>
    </div>
</div>

<div class="site-footer">
    <footer class="site-info" >
        <div>
            <div class="copyright-bar">
                Â© Hello World <br />
                <a href="/contact/">Contact</a> -
                <a href="/login/">Login</a>
            </div>
        </div>
    </footer>
</div>

</body>
</html>
